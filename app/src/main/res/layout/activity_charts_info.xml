<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.suhas.us.csci571homework9.ChartsInfo">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:text=""
        android:id="@+id/hstZestimate"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:visibility="invisible"
        android:textSize="17dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/address"
        android:layout_below="@+id/hstZestimate"
        android:visibility="invisible"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_below="@+id/address"
        android:visibility="visible"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="99dp"
        android:layout_height="40dp"
        android:text="@string/prev"
        android:id="@+id/prev"
        android:visibility="invisible"
        android:layout_alignTop="@+id/next"
        android:layout_marginRight="12dp"
        android:layout_marginLeft="62dp" />


    <Button
        android:layout_width="99dp"
        android:layout_height="40dp"
        android:text="@string/next"
        android:id="@+id/next"
        android:visibility="invisible"
        android:layout_below="@+id/imageView3"
        android:layout_toRightOf="@+id/prev"
        android:layout_toEndOf="@+id/prev"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="false"
        android:layout_alignParentBottom="false"
        android:layout_alignParentLeft="false"
        android:layout_alignWithParentIfMissing="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="\u00a9Zillow,Inc.2006-2014"
        android:layout_below="@+id/next"
        android:id="@+id/footer1"
        android:layout_gravity="center"
        android:layout_centerHorizontal="true"
        android:visibility="invisible" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/footer1"
        android:id="@+id/footer2"
        android:layout_gravity="center"
        android:linksClickable="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/footer2"
        android:id="@+id/footer3"
        android:layout_gravity="center"
        android:linksClickable="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible" />




</RelativeLayout>
